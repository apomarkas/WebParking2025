@model Dictionary<User,string>

@{
    
}

@if (Model.Any())
{
    foreach (var user in Model)
    {
        <div class="card mb-3">
            <div class="card-header">
                User name : @user.Key.UserName
            </div>
            <div class="card-body">
                <h5 class="card-title">@user.Value</h5>
            </div>            
        </div>
    }
}